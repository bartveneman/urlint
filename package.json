{
  "name": "urlint",
  "description": "Get all the links behind an url and classify them based on HTTP status code.",
  "homepage": "https://documentup.com/urlint/urlint",
  "version": "1.0.1",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urlint/urlint.git"
  },
  "bugs": {
    "url": "https://github.com/urlint/urlint/issues"
  },
  "keywords": [
    "classify",
    "http",
    "lint",
    "status",
    "url",
    "validate"
  ],
  "dependencies": {
    "aigle": "~1.10.1",
    "got": "~8.0.3",
    "html-urls": "~1.0.1",
    "mitt": "~1.1.3",
    "url-code-status": "~1.0.0"
  },
  "devDependencies": {
    "coveralls": "latest",
    "finepack": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "test": "exit 0"
  },
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
